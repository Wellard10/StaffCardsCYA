	tablePermission 'Staff ID' =
			[Staff Name] IN
			
			    CALCULATETABLE(
			
			        VALUES('User Access'[StaffName]),
			
			        FILTER(
			
			            'User Access',
			
			            LOWER('User Access'[Staff Email]) = LOWER(USERPRINCIPALNAME())
			
			        )
			
			    )

